{
  "name": "expo-running-chatgpt-gmaps",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.19.4"
  },
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@env$": "<rootDir>/__mocks__/@env.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-clone-referenced-element|@react-native-community|@testing-library)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@googlemaps/markerclusterer": "^2.5.1",
    "@react-google-maps/api": "2.20.5",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "react-test-renderer": "19.1.0",
    "@types/google.maps": "^3.58.1",
    "@vis.gl/react-google-maps": "latest",
    "babel-plugin-module-resolver": "^4.1.0",
    "date-fns": "4.1.0",
    "dotenv": "^16.4.7",
    "expo": "^54.0.0",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "lucide-react-native": "0.483.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-dotenv": "3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "1.11.0",
    "react-native-google-places-autocomplete": "2.5.1",
    "react-native-google-places-textinput": "0.8.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "vite": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.16",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.3.3"
  },
  "private": true
}
